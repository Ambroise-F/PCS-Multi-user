#include <stdio.h>
#include <math.h>
#include <gmp.h>
#include <inttypes.h>




int test1()
{
  mpz_t a,b;
  int r;
  mpz_init(a);
  mpz_init_set_ui(b,15);
  gmp_printf("%Zd,%Zd\n",a,b);
  r = mpz_mmod_ui(a,b,10);
  gmp_printf("%Zd,%Zd\n",a,b);
  printf("%d\n",r);
  return r;
}

int test2()
{
  mpz_t* a;
  mpz_t  b;
  int r;
  mpz_init(*a);
  mpz_init_set_ui(b,15);
  gmp_printf("%Zd,%Zd\n",*a,b);
  r = mpz_mmod_ui(*a,b,10);
  gmp_printf("%Zd,%Zd\n",*a,b);
  printf("%d\n",r);
  return r;
}


int test3()
{
  mpz_t a,a2,n;
  mpz_inits(a,a2,n,NULL);
  mpz_init_set_ui(a,61);
  mpz_init_set_ui(a2,99);
  mpz_init_set_ui(n,63);
  mpz_add(a, a2, a2); // => intuition a = a2+a2
  gmp_printf("a=%Zd , a2=%Zd\n",a,a2); 
  mpz_mmod(a, a2, n); // => intuition a = a2%n
  gmp_printf("a=%Zd , a2=%Zd , n=%Zd\n",a,a2,n);
}

